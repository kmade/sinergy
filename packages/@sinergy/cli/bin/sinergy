#!/usr/bin/env node

process.title = "sinergy"
const semver = require("semver")
const getopts = require("getopts")

// Exit early if the user"s node version is too low.
if (!semver.satisfies(process.version, ">=6")) {
  process.stdout.write("Please use nodejs version >= 6\n");
  process.exit(1);
}
const args = {
  alias: {
    s: "super",
  },
  default: {
    turbo: true
  }
}
const options = getopts(process.argv.slice(2), args)

